<div class="movie-view">
  <h1>Ironhack Cinema</h1> 
  <p>Click on the movie to check the showtimes!</p>
  <br>
  <div class="movie-container">
    {{#each movies}}
      <div class="movie-div">
        <img class="movie-image" src="{{image}}" alt="">
        <p>{{title}}</p>
        <a href="/movie/{{_id}}">See more</a>
      </div>
    {{/each}}
  </div>
</div>